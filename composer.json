{
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.8",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "CyberDimon\\": "src/",
            "ThirdParty\\": "thirdparty/src/"
        }
    },
    "scripts": {
        "check-style": "phpcs --standard=PSR12 --ignore=vendor/ ./",
        "fix-style": "phpcbf --standard=PSR12 --ignore=vendor/ ./"
    }
}
